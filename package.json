{
  "name": "animeback-submit",
  "version": "1.0.0",
  "description": "submission system for animeback extensions",
  "main": "main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "node script/clean",
    "build:resize": "node script/resize",
    "build:dates": "node script/dates",
    "build:colors": "node script/colors",
    "build:categories": "node script/categories",
    "build:releases": "node script/releases",
    "build:readmes": "node script/readmes",
    "build:pack": "node script/pack",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "prettier": "prettier --check \"./**/*.{ts,js,json}\"",
    "test": "mocha --reporter min test/human-data.js test/colors-spec.js && prettier --check \"./**/*.{ts,js,json}\" --write",
    "test:debug": "mocha --reporter spec test/human-data.js test/colors-spec.js && prettier --check \"./**/*.{ts,js,json}\" --write",
    "pretest-all": "npm run build",
    "test-all": "mocha --reporter min && prettier --check \"./**/*.{ts,js,json}\" --write",
    "wizard": "node wizard.js",
    "release": "script/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaiStudio/animeback-submit.git"
  },
  "keywords": [
    "tai",
    "studio",
    "animeback",
    "extension",
    "submission"
  ],
  "author": "Tai Studio <tai.studio@outlook.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaiStudio/animeback-submit/issues"
  },
  "homepage": "https://github.com/TaiStudio/animeback-submit#readme",
  "devDependencies": {
    "@octokit/rest": "^18.5.3",
    "bottleneck": "^1.16.0",
    "chai": "^4.3.4",
    "check-for-leaks": "^1.2.1",
    "cheerio": "^0.22.0",
    "clean-deep": "^3.4.0",
    "color-convert": "^2.0.1",
    "count-array-values": "^1.2.1",
    "dotenv-safe": "^8.2.0",
    "fs": "0.0.1-security",
    "get-image-colors": "^4.0.0",
    "github-url-to-object": "^4.0.6",
    "human-interval": "^2.0.1",
    "image-size": "^1.0.0",
    "imagemin": "^6.1.0",
    "inquirer": "^8.0.0",
    "is-svg": "^4.2.2",
    "is-url": "^1.2.4",
    "jimp": "^0.16.1",
    "make-color-accessible": "^1.2.0",
    "mkdirp": "^0.5.5",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "pick-a-good-color": "^1.1.1",
    "prettier": "^2.2.1",
    "readdirp": "^3.5.0",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^3.0.2",
    "sharp": "^0.28.1",
    "sinon": "^10.0.0",
    "slugg": "^1.2.1",
    "tinycolor2": "^1.4.2",
    "url": "^0.11.0"
  },
  "dependencies": {
    "json-stable-stringify": "^1.0.1",
    "mime-types": "^2.1.30",
    "rev-hash": "^3.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
